
reg temp_time_stamp_ok;

always@(negedge clk_r2)
begin
if(|repeaterR[27:4])begin temp_time_stamp_ok<=1'b1; end
else begin temp_time_stamp_ok<=1'b0; end
end

wire time_stamp_ok;
assign time_stamp_ok=temp_time_stamp_ok;
//assign time_stamp_ok=(|repeaterR[27:4]);





  pulse pulse1V
  (
.antennaI(antennaI1V[7:0]),
